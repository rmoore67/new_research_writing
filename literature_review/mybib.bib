@Article{Skliris2016,
author={Skliris, Nikolaos
and Zika, Jan D.
and Nurser, George
and Josey, Simon A.
and Marsh, Robert},
title={Global water cycle amplifying at less than the Clausius-Clapeyron rate},
journal={Scientific Reports},
year={2016},
month={Dec},
day={09},
volume={6},
number={1},
pages={38752},
abstract={A change in the cycle of water from dry to wet regions of the globe would have far reaching impact on humanity. As air warms, its capacity to hold water increases at the Clausius-Clapeyron rate (CC, approximately 7{\%}{\thinspace}{\textdegree}C−1). Surface ocean salinity observations have suggested the water cycle has amplified at close to CC following recent global warming, a result that was found to be at odds with state-of the art climate models. Here we employ a method based on water mass transformation theory for inferring changes in the water cycle from changes in three-dimensional salinity. Using full depth salinity observations we infer a water cycle amplification of 3.0{\thinspace}{\textpm}{\thinspace}1.6{\%}{\thinspace}{\textdegree}C−1 over 1950--2010. Climate models agree with observations in terms of a water cycle amplification (4.3{\thinspace}{\textpm}{\thinspace}2.0{\%}{\thinspace}{\textdegree}C−1) substantially less than CC adding confidence to projections of total water cycle change under greenhouse gas emission scenarios.},
issn={2045-2322},
doi={10.1038/srep38752},
url={https://doi.org/10.1038/srep38752}
}


